/* ============================================
   Agent Overview Stylesheet
   ============================================ */

QFrame#AgentOverview {
  background-color: #1b262c;
  border: 1px solid #334e68;
  border-radius: 8px;
  padding: 12px;
}

QFrame#AgentOverview > QFrame, 
QFrame#AgentOverview > QLabel,
QLabel {
  background-color: #1b262c;
}

QLabel#AgentOverviewHeader {
  color: #ffffff;
  font-weight: bold;
  font-size: 14px;
  margin-bottom: 8px;
}

QFrame#AgentCard {
  background-color: #0f4c75;
  border: 1px solid #364153;
  border-radius: 4px;
  padding: 6px;
}

QFrame#AgentCard QLabel {
  color: #e0e0e0;
}